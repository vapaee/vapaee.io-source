{
  "name": "vapaee.io-source",
  "version": "v3.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --poll=2000 -o",
    "serve": "ng serve --poll=2000 -o",

    "libs": "npm run lib-feedback && npm run lib-style && npm run lib-scatter && npm run lib-rex && npm run lib-dex",

    "lib-rex": "ng build @vapaee/rex --prod",
    "lib-dex": "ng build @vapaee/dex --prod",
    "lib-style": "ng build @vapaee/style --prod",
    "lib-scatter": "ng build @vapaee/scatter --prod",
    "lib-feedback": "ng build @vapaee/feedback --prod",

    "build": "ng build --aot --buildOptimizer --prod",
    "deploy": "npm run libs && ng build --aot --buildOptimizer --prod && cp .htaccess /var/www/vapaee.io/.htaccess && cp cert/* /var/www/vapaee.io/ && cd /var/www/vapaee.io && git add -A && git commit -m 'save' && git push && amazon_server_console",
    "postinstall": "node src/patch.js; npm run libs; cd contracts/_examples; if [ ! -d 'eosio.contracts' ]; then git clone https://github.com/Telos-Foundation/eosio.contracts; fi; npm run serve",
    "save": "git add -A; git commit -m 'save'; git push;",
    "pull": "git pull; cp ../airdrop-on-telos/* ./airdrop-on-telos/ -R;",
    "prod": "git checkout prod && git merge master -m 'merge master' && ng build --aot --buildOptimizer --prod && git add -A && git commit -m 'update' && git push && amazon_server_console;"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/animations": "^6.1.10",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/pwa": "^0.800.2",
    "@angular/router": "^6.0.3",
    "@angular/service-worker": "^6.1.10",
    "@ng-bootstrap/ng-bootstrap": "^4.2.1",
    "bignumber.js": "^8.0.2",
    "bootstrap": "^4.3.1",
    "bytebuffer": "^5.0.1",
    "core-js": "^2.5.4",
    "eosjs": "^16.0.9",
    "fast-csv": "^2.4.1",
    "fstream": "^1.0.12",
    "jdenticon": "^2.1.0",
    "jquery": "^3.4.0",
    "ng-sidebar": "^8.0.0",
    "ng2-google-charts": "^4.0.0",
    "ng2-search-filter": "^0.4.7",
    "ngx-cookie-service": "^2.1.0",
    "ngx-markdown": "^6.1.0",
    "ngx-perfect-scrollbar": "^7.2.1",
    "popper.js": "^1.14.7",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.3.3",
    "scatterjs-core": "^2.3.8",
    "scatterjs-plugin-eosjs": "^1.3.10",
    "zone.js": "^0.8.26"
  },
  "notas": {
    "scatterjs-core": "^2.3.8",
    "scatterjs-plugin-eosjs": "^1.3.10",
    "eosjs_": "^16.0.9",
    "@scatterjs/core": "^2.7.26",
    "@scatterjs/eosjs2": "^1.5.8",
    "@scatterjs/lynx": "^1.6.17",
    "eosjs": "^20.0.0"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.3",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular-devkit/build-angular": "~0.6.8",
    "ng-packagr": "^3.0.0-rc.2",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.8",
    "@angular/language-service": "^6.0.3",
    "@babel/runtime": "^7.1.5",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
}