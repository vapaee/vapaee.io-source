{
  "name": "vapaee.io-source",
  "version": "v3.8.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --poll=2000 -o",
    "serve": "ng serve --poll=2000 -o",
    "buildprod": "ng build --aot --buildOptimizer --prod",
    "build": "ng build",
    "deploy": "npm run deploynow",
    "prepare-website": "if [ ! -d './vapaee.io' ]; then git clone https://github.com/vapaee/vapaee.io-website.git ./vapaee.io; fi; cd ./vapaee.io && git pull && cd ..;",
    "deploynow": "npm run prepare-website; npm run buildprod && npm run cpbasic && npm run copycert && cd ./vapaee.io/www && npm run save && amazon_server_console",
    "cpbasic": "cp .htaccess ./vapaee.io/www/.htaccess",
    "copycert": "cp cert/* ./vapaee.io/www/",
    "postinstall": "node src/patch.js;",
    "full": "npm i; npm run libs; cd contracts/_examples; if [ ! -d 'eosio.contracts' ]; then git clone https://github.com/Telos-Foundation/eosio.contracts; fi; npm run serve",
    "save": "git add -A && git commit -m 'save' && git push",
    "pull": "git pull; cp ../airdrop-on-telos/* ./airdrop-on-telos/ -R;",
    "prod": "git checkout prod && git merge master -m 'merge master' && npm run buildprod && git add -A && git commit -m 'update' && git push && amazon_server_console;"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "0.1100.5",
