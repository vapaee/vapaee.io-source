{
  "name": "vapaee-io",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --poll=2000 -o",
    "serve": "npm run vapaee-libs && ng serve --poll=2000 -o",
    "buildprod": "ng build --aot --build-optimizer",
    "build": "ng build",
    "deploy": "npm run deploynow",
    "prepare-website": "if [ ! -d './vapaee.io' ]; then git clone https://github.com/vapaee/vapaee.io-website.git ./vapaee.io; fi; cd ./vapaee.io && git pull && cd ..;",
    "deploynow": "npm run prepare-website; npm run buildprod && npm run cpbasic && npm run copycert && cd ./vapaee.io/www && npm run save && amazon_server_console",
    "cpbasic": "cp .htaccess ./vapaee.io/www/.htaccess",
    "copycert": "cp cert/* ./vapaee.io/www/",
    "vapaee-libs": "./scripts/get_libs.sh",
    "postinstall": "npm run fix-stream; npm run fix-types; npm run fix-libs; ng build",
    "fix-stream": "echo 'node ./scripts/stream_patch.js; SKKIPED'",
    "fix-types": "./scripts/types.sh",
    "fix-libs": "./scripts/get_libs.sh force",
    "full": "npm i; npm run libs; cd contracts/_examples; if [ ! -d 'eosio.contracts' ]; then git clone https://github.com/Telos-Foundation/eosio.contracts; fi; npm run serve",
    "save": "git add -A && git commit -m 'save' && git push",
    "pull": "git pull; cp ../airdrop-on-telos/* ./airdrop-on-telos/ -R;"    
  },
  "private": true,
  "temp": {},
  "dependencies": {
    "@angular/animations": "12.2.16",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/forms": "12.2.16",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/pwa": "12.2.16",
    "@angular/router": "12.2.16",
    "@angular/service-worker": "12.2.16",
    "@ng-bootstrap/ng-bootstrap": "4.2.1",
    "@vapaee/core": "1.0.0",
    "@vapaee/dex": "2.6.1",
    "@vapaee/feedback": "1.2.1",
    "@vapaee/idp-anchor": "0.0.1",
    "@vapaee/rex": "0.3.3",
    "@vapaee/styles": "0.2.2",
    "@vapaee/wallet": "1.1.1",
    "anchor-link": "3.3.8",
    "anchor-link-browser-transport": "3.2.5",
    "animate.css": "4.1.1",
    "bignumber.js": "9.0.1",
    "bootstrap": "5.1.1",
    "eosjs": "21.0.3",
    "jdenticon": "2.2.0",
    "jquery": "3.3.1",
    "long": "4.0.0",
    "ng-sidebar": "8.0.0",
    "ng2-google-charts": "4.0.0",
    "ng2-search-filter": "0.4.7",
    "ngx-cookie-service": "2.4.0",
    "ngx-markdown": "6.1.0",
    "ngx-perfect-scrollbar": "7.2.1",
    "ngx-smart-modal": "^7.4.1",
    "popper.js": "1.14.7",
    "rxjs": "6.6.0",
    "rxjs-compat": "6.3.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.1.2",
    "@angular/cli": "12.2.16",
    "@angular/compiler-cli": "12.2.16",
    "@types/bn.js": "5.1.0",
    "@types/jasmine": "3.8.0",
    "@types/long": "4.0.1",
    "@types/node": "12.11.1",
    "jasmine-core": "3.8.0",
    "karma": "6.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "typescript": "4.3.5"
  },
  "browser": {
    "crypto": false
  }
}
